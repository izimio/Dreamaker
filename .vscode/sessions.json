{
    "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v10/terminal-keeper.json",
    "theme": "tribe",
    "active": "dev-backend",
    "activateOnStartup": true,
    "keepExistingTerminals": false,
    "sessions": {
        "dev-backend": [
            {
                "name": "home",
                "autoExecuteCommands": true,
                "icon": "person",
                "color": "terminal.ansiGreen",
                "commands": [
                    "echo hello"
                ]
            },
            [
                {
                    "name": "backend",
                    "focus": true,
                    "autoExecuteCommands": true,
                    "commands": [
                        "cd backend"
                    ]
                },
                {
                    "name": "backend scripts",
                    "autoExecuteCommands": true,
                    "commands": [
                        "cd backend"
                    ]
                }
            ],
            [
                {
                    "name": "node",
                    "cwd": "contracts",
                    "autoExecuteCommands": true,
                    "commands": [
                        "npm run node"
                    ]
                },
                {
                    "name": "deployer & scripts",
                    "autoExecuteCommands": true,
                    "cwd": "contracts",
                    "commands": [
                        "sleep 1",
                        "npm run tdeploy"
                    ]
                }
            ]
        ],
        "default": [
            {
                "name": "home",
                "commands": [
                    ""
                ]
            }
        ]
    }
}